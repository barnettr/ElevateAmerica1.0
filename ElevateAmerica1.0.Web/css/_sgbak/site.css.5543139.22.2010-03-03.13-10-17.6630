/*Navigation*/

	#navBar
	{		
		width: 966px;
		margin: 0 14px 7px 60px;
		padding: 7px 0 0 0;
		height: 30px;
	}

	#navBar ul
	{
		margin: 0;
		padding: 0;
		list-style-type: none;
	}

	#navBar ul li
	{
		display: inline;
		float: left;
		height: 30px;
		text-indent: -2000px;
	}

	#navBar ul li a
	{
		height: 30px;
		outline: none;
	}

	/*Each Nav Item*/
		
		/*Home*/
		#navBar ul li#Home a
		{
			width: 65px;
			display: block;
			background:  url(../assets/navBar.gif);
			outline: none;
		}
	
	
		#navBar ul li#Home a:hover 
		{
			background-position: 0 -30px;
		}
	
		body.home #navBar ul li#Home a, body.home #navBar ul li#Home a:hover
		{
			background-position: 0px -60px;
			outline: none;
		}
		
		/*Getting Started*/
		#navBar ul li#GettingStarted a
		{
			width: 121px;
			display: block;
			background:  url(../assets/navBar.gif);
			background-position: -65px 0px;
			outline: none;
		}
	
	
		#navBar ul li#GettingStarted a:hover 
		{
			background-position: -65px -30px;
		}
	
		body.GettingStarted #navBar ul li#GettingStarted a, body.GettingStarted #navBar ul li#GettingStarted a:hover
		{
			background-position: -65px -60px;
			outline: none;
		}		

		/*Expand Your Skills*/
		#navBar ul li#ExpandYourSkills a
		{
